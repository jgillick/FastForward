{
  "name": "fastforward",
  "version": "1.0.0",
  "description": "A simple go links application written in nextjs, nexus-future and prisma.",
  "scripts": {
    "dev": "npx ts-node ./server/index.ts",
    "build": "tsc -pretty -p . && npx next build ./web",
    "start": "NODE_ENV=production npx ts-node ./server/index.ts",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only backend/graphql",
    "prisma:generate": "npx prisma2 generate --schema=./backend/prisma/schema.prisma",
    "prisma:migrations": "npx prisma2 migrate up --experimental --schema=backend/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "Jeremy Gillick",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.38",
    "@apollo/react-ssr": "^3.1.3",
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.44",
    "@prisma/client": "^2.0.0-preview023",
    "apollo-boost": "^0.4.7",
    "apollo-server-express": "^2.11.0",
    "date-fns": "^2.10.0",
    "express": "^4.17.1",
    "google-auth-library": "^5.10.1",
    "graphql": "^14.6.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "^9.2.2",
    "nexus": "^0.12.0-rc.13",
    "nexus-prisma": "^0.11.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-login": "^5.1.1",
    "recharts": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@types/gapi": "0.0.39",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@zeit/next-sass": "^1.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "node-sass": "^4.13.1",
    "prisma2": "^2.0.0-preview024",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
